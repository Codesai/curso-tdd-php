{
    "name": "codesai/inspiration-of-the-day",
    "description": "Inspiration of the day Kata",
    "config": {
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "9.2.15",
        "infection/infection": ">=0.26.13",
        "phpspec/prophecy-phpunit": "^2.2"
    },
    "authors": [
        {
            "name": "Codesai",
            "email": "contact@codesai.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Codesai\\TDD\\InspirationOfTheDay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
