{
    "name": "codesai/coffee_machine",
    "description": "Coffee Machine",
    "config": {
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "9.2.15",
        "infection/infection": ">=0.26.13"
    },
    "authors": [
        {
            "name": "Codesai",
            "email": "contact@codesai.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Codesai\\TDD\\CoffeeMachine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
